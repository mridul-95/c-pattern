#include<stdio.h>
int main()
{
 int n,c=0,d,min;
 scanf("%d",&n);
 int a[n];
 for(int i=0;i<n;i++)
 {
     if(i==1)
     min=a[i];
     scanf("%d",&a[i]);
     if(a[i]<min)
     min=a[i];
 }
 while(d!=n)
 {
     c=0;
 for(int i=0;i<n;i++)
 {
       if(a[i]==0)
       c++;
     if(a[i]!=0)
   a[i]=a[i]-min;
  // printf("%d ",a[i]);
 
 }
 if((n-c)!=0)
 printf("\n%d",n-c);
  d=c;
 for(int i=0;i<n;i++)
 {
     if(a[i]!=0)
     if(a[i]<min)
     a[i]=min;
 }
}
}
